{
  "name": "sekai-viewer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sekai.best/",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/data-grid": "^4.0.0-alpha.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reduxjs/toolkit": "^1.4.0",
    "@rumess/react-flip-countdown": "^1.1.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cron": "^1.7.2",
    "@types/howler": "^2.2.1",
    "@types/humanize-duration": "^3.18.1",
    "@types/jest": "^26.0.15",
    "@types/modernizr": "^3.5.3",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/wicg-mediasession": "^1.1.0",
    "axios": "^0.20.0",
    "cron": "^1.8.2",
    "howler": "^2.2.1",
    "humanize-duration": "^3.24.0",
    "i18next": "^19.8.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-fetch-backend": "^3.0.0",
    "localforage": "^1.9.0",
    "mdi-material-ui": "^6.20.0",
    "p-queue": "^6.6.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-gtm-module": "^2.0.11",
    "react-i18next": "^11.7.3",
    "react-infinite-scroll-component": "^5.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.4",
    "react-viewer": "^3.2.2",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.9.7",
    "webp-hero": "0.0.0-dev.27"
  },
  "scripts": {
    "start": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "exti18n": "i18next -c ./i18next-parser.config.js -s",
    "lint": "eslint --fix **/*.{js,jsx,ts,tsx}",
    "commit": "cz"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "http-proxy-middleware": "^1.0.6",
    "i18next-parser": "^3.3.0",
    "prettier": "^2.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": true,
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes"
        },
        "docs": {
          "description": "Documentation only changes",
          "title": "Documentation"
        },
        "style": {
          "description": "Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc)",
          "title": "Styles"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring"
        },
        "i18n": {
          "description": "Changes regarding i18n (locale) files",
          "title": "I18n (Locale)"
        },
        "perf": {
          "description": "A code change that improves performance",
          "title": "Performance Improvements"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Tests"
        },
        "build": {
          "description": "Changes that affect the build system or external dependencies (example scopes: gulp, broccoli, npm)",
          "title": "Builds"
        },
        "ci": {
          "description": "Changes to our CI configuration files and scripts (example scopes: Travis, Circle, BrowserStack, SauceLabs)",
          "title": "Continuous Integrations"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores"
        },
        "revert": {
          "description": "Reverts a previous commit",
          "title": "Reverts"
        }
      }
    }
  }
}
